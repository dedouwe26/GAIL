{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "GAIL: Build",
			"group": "build",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/GAIL.csproj"]
		},
		{
			"label": "GAIL: Package",
			"dependsOn": "GAIL: Test",
			"group": "build",
			"command": "dotnet",
			"args": ["pack", "${workspaceFolder}/GAIL.csproj"]
		},
		{
			"label": "GAIL: Test",
			"group": "test",
			"command": "dotnet",
			"args": ["test", "${workspaceFolder}/Tests/GAIL.Tests.csproj"]
		},
		{
			"label": "GAIL Networking: Build",
			"group": "build",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/GAIL.Networking.csproj"]
		},
		{
			"label": "GAIL Networking: Package",
			"dependsOn": "GAIL Networking: Test",
			"group": "build",
			"command": "dotnet",
			"args": ["pack", "${workspaceFolder}/GAIL.Networking.csproj"]
		},
		{
			"label": "GAIL Networking: Test",
			"group": "test",
			"command": "dotnet",
			"args": ["test", "${workspaceFolder}/Tests/GAIL.Networking.Tests.csproj"]
		},
		{
			"label": "GAIL Storage: Build",
			"group": "build",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/GAIL.Storage.csproj"]
		},
		{
			"label": "GAIL Storage: Package",
			"dependsOn": "GAIL Storage: Test",
			"group": "build",
			"command": "dotnet",
			"args": ["pack", "${workspaceFolder}/GAIL.Storage.csproj"]
		},
		{
			"label": "GAIL Storage: Test",
			"group": "test",
			"command": "dotnet",
			"args": ["test", "${workspaceFolder}/Tests/GAIL.Storage.Tests.csproj"]
		},
		{
			"label": "Build examples.Packets.Server",
			"group": "build",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/examples/Packets/Server/Server.csproj"]
		},
		{
			"label": "Build examples.Packets.Client",
			"group": "build",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/examples/Packets/Client/Client.csproj"]
		},
		{
			"label": "Build examples.Packets.Client2",
			"group": "build",
			"command": "dotnet",
			"args": ["build", "${workspaceFolder}/examples/Packets/Client2/Client2.csproj"]
		}
	]
}