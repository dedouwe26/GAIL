{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "cppbuild",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/MT",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "GAILBindings/main.cpp",
                "${workspaceFolder}/GAILBindings/lib/glfw/lib/glfw3_mt.lib",
                "${workspaceFolder}/GAILBindings/lib/glfw/lib/glfw3dll.lib",
                "${workspaceFolder}/GAILBindings/lib/glad/lib/glad.c",
                "User32.lib",
                "Gdi32.lib",
                "Shell32.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}